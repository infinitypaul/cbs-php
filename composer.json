{
    "name": "infinitypaul/cbs-php",
    "description": "A PHP Package for working with central billing system seamlessly",
    "keywords": [
        "infinitypaul",
        "cbs-php"
    ],
    "homepage": "https://github.com/infinitypaul/cbs-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paul Edward",
            "email": "infinitypaul@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.5",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Infinitypaul\\Cbs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infinitypaul\\Cbs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
